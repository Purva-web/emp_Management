PROCEDURE "ADDRESS_DATA" (
    IN ST_ADDRESS_DATA "ST_ADDRESS_DATA",
    OUT OUT_SUCCESS VARCHAR(100)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
    INSERT INTO "EMPMGMT_ADDRESS_DETAILS"
    ("PINCODE", "CITY", "STATE", "LANDMARK")
    SELECT "PINCODE", "CITY", "STATE", "LANDMARK"
    FROM :ST_ADDRESS_DATA;

    COMMIT;
    OUT_SUCCESS := 'SUCCESS';
END